 while (cnpj2.indexOf(".") > -1){
  cnpj2 = cnpj2.replace(".","");
 }

 while (cnpj2.indexOf("-") > -1){
  cnpj2 = cnpj2.replace("-","");
 }

 while (cnpj2.indexOf("/") > -1){
  cnpj2 = cnpj2.replace("/","");
