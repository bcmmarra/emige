function strPad(palavra, casas, carac, dir) { 
 //dir = 'R' => Right; dir = 'L' => Left; 
 if(palavra == null || palavra == '') palavra = 0; 
 var ret = ''; 
 var nro = casas - (palavra.length); 
 for(var i = 0; i < nro; i++) ret += carac; 
 if(dir == 'R') 
   ret = palavra + ret; 
 else if(dir == 'L') 
   ret += palavra; 
 return ret; 
}

